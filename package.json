{
  "name": "wdocs",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "type": "module",
  "scripts": {
    "prepare": "lefthook install",
    "postinstall": "taze -r -w -i",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "build": "turbo build",
    "format": "turbo format",
    "test": "turbo test",
    "verify": "turbo verify",
    "start": "turbo watch verify",
    "update:deps": "taze -r -w -i"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "lefthook": "^1.13.6",
    "taze": "^0.16.9",
    "turbo": "^2.7.1",
    "typescript": "^5.9.3"
  }
}

{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".output/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "verify": {
      "dependsOn": ["^verify"],
      "outputs": []
    },
    "clean": {
      "dependsOn": [],
      "outputs": []
    }
  }
}
